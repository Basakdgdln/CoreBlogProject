@model List<EntityLayer.Concrete.Comment><div class="row">    <div class="col-lg-12">        <h2>Yorumlar:</h2>        @foreach (var x in Model)        {<div class="social-feed-box">    <div class="social-avatar">        <a href="" class="float-left">            <img alt="image" src="~/inspinia-gh-pages/img/a1.jpg">        </a>        <div class="media-body">            <a href="#">              @x.CommentUsername            </a>            <small class="text-muted">@x.CommentDate</small>        </div>    </div>    <div class="social-body">        <p>          @x.CommentContent        </p>    </div></div>}    </div></div>